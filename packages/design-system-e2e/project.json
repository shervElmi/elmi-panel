{
  "root": "packages/design-system-e2e",
  "sourceRoot": "packages/design-system-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "packages/design-system-e2e/cypress.json",
        "devServerTarget": "design-system:storybook"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "design-system:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/design-system-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["design-system"]
}
